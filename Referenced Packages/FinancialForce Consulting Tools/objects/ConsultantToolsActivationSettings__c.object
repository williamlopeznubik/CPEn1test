<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>Determines whether activation is required for Consulting Tools and the domain or email address that the activation code is sent to.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>ActivationEmailTarget__c</fullName>
        <defaultValue>&quot;@financialforce.com&quot;</defaultValue>
        <deprecated>false</deprecated>
        <description>Domain or email address to which activation email is to be sent. The default value is @financialforce.com.</description>
        <externalId>false</externalId>
        <label>Activation Email Target</label>
        <length>80</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ActivationRequired__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>Determines whether users must activate the Consulting Tools package before they can use the tools. The default value is true.</description>
        <externalId>false</externalId>
        <label>Activation Required</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>Consultant Tools Activation Settings</label>
</CustomObject>
