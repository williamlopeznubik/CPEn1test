						<apex:image styleClass="absmiddle" value="{!URLFOR($Resource.c2g__coda2go,'coda2go/web/images/sort_desc_arrow.gif')}" rendered="{!AND(viewstate.sortCol=='Column4',viewstate.sortDir=='desc')}"/>
					</td>
