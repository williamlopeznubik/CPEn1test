			<tbody>
			<apex:repeat value="{!viewstate.table.rows}" var="row">
				<tr class="{!row.style}">
				<apex:repeat value="{!row.cells}" var="cell">
					<td class="{!cell.style}" colSpan="{!cell.colSpan}">{!cell.value}&nbsp;</td>
				</apex:repeat>
				</tr>
			</apex:repeat>
