						<apex:commandLink value="{!viewstate.agedDef.column8Name}" action="{!sortTable}" onclick="CODA.indicatorOn('{!$Label.codaPageAgedAnalysisReportSortingData12}')" oncomplete="CODA.indicatorOff()" reRender="report">
