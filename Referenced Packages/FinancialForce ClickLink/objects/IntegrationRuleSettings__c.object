<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>FinancialForce ClickLink settings</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>IncludeInfoMessagesInLogs__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Controls the output of information messages related to each record ClickLink Rules processes. This setting generates a large amount of Log records that needs clearly back manually.</description>
        <externalId>false</externalId>
        <label>Include Information Messages in Logs</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>LicenseKey__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>License Key</label>
        <length>64</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>SupportMode__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Enables some unexpected errors to be submitted back to FinancialForce.com for more detailed analysis.</description>
        <externalId>false</externalId>
        <label>Support Mode</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>SyncEnabled__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>Indicates whether the ClickLink synchronization feature is enabled for the organization. When false, the ClickLink synchronization feature is disabled for the whole organization. The default value is true.</description>
        <externalId>false</externalId>
        <label>Sync Enabled</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>ClickLink Settings</label>
</CustomObject>
